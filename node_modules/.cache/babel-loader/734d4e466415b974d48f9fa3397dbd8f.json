cAAA;AACA,UAAA,UAAA,GAAA,EAAA,CAAA,IAAA,GACA,GADA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,MAAA;AAAA,OADA,EAEA,OAFA,CAEA,QAFA,CAAA;AAGA,MAAA,OAAA,CAAA,GAAA,CAAA,UAAA;AACA,aAAA,UAAA;AACA,KAdA;AAeA,IAAA,cAfA,4BAeA;AACA,UAAA,IAAA,GAAA,KAAA,QAAA;AACA,UAAA,aAAA,GAAA,IAAA,CAAA,MAAA,CAAA,UAAA,MAAA;AAAA,eAAA,MAAA,CAAA,GAAA,KAAA,OAAA,IAAA,MAAA,CAAA,MAAA,KAAA,aAAA;AAAA,OAAA,CAAA;AACA,aAAA,aAAA;AACA;AAnBA;AA3BA,CAAA","sourcesContent":["<template>\n  <div class=\"visualization\" id=\"carriers\">\n    <svg width=\"90%\" height=\"100%\">\n      <g :transform=\"'translate('+ margin.left + ',' + margin.top + ')'\">\n        <rect :width=\"width\" :height=\"height\" x=\"0\" y=\"0\" fill=\"lightblue\" :data='filterCarriers'/>\n      </g>\n    </svg>\n  </div>\n</template>\n\n<script>\nimport * as d3 from 'd3'\n\n// data\nimport CarriersReport from '../assets/data/world_regional_report.json'\n\nexport default {\n  name: 'EnergyCarriers',\n  props: {\n    width: {\n      type: Number,\n      required: true\n    },\n    height: {\n      type: Number,\n      required: true\n    },\n    step: {\n      type: Number,\n      default: 0\n    }\n  },\n  data () {\n    return {\n      CarriersReport,\n      margin: {\n        left: 70,\n        top: 30,\n        bottom: 30,\n        right: 30\n      }\n    }\n  },\n  computed: {\n    innerWidth (){\n      return this.width - this.margin.left - this.margin.right\n    },\n    innerHeight (){\n      return this.height - this.margin.top - this.margin.bottom\n    },\n    dataNest () {\n      const carriers = this.CarriersReport\n      const nesteddata = d3.nest()\n        .key(d => d.region)\n        .entries(carriers)\n      console.log(nesteddata)\n      return nesteddata\n    },\n    filterCarriers () {\n      const data = this.dataNest\n      const dataSelection = data.filter(region => region.key === 'World' && region.values === 'electricity')\n      return dataSelection\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\n.visualization {\n  width: inherit;\n  height: inherit;\n}\n\n</style>\n"],"sourceRoot":"src/components"}]}