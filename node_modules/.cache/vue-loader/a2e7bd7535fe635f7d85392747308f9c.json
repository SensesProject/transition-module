\nimport CarriersReport from '../assets/data/world_regional_report.json'\n\nexport default {\n  name: 'EnergyCarriers',\n  props: {\n    width: {\n      type: Number,\n      required: true\n    },\n    height: {\n      type: Number,\n      required: true\n    },\n    step: {\n      type: Number,\n      default: 0\n    }\n  },\n  data () {\n    return {\n      CarriersReport,\n      margin: {\n        left: 70,\n        top: 30,\n        bottom: 30,\n        right: 30\n      }\n    }\n  },\n  computed: {\n    innerWidth (){\n      return this.width - this.margin.left - this.margin.right\n    },\n    innerHeight (){\n      return this.height - this.margin.top - this.margin.bottom\n    },\n    dataNest () {\n      const carriers = this.CarriersReport\n      const nesteddata = d3.nest()\n        .key(d => d.region)\n        .entries(carriers)\n      console.log(nesteddata)\n      return nesteddata\n    },\n    filterCarriers () {\n      const data = this.dataNest\n      const dataSelection = data.filter(region => region.key === 'World' && region.values === 'electricity')\n      return dataSelection\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\n.visualization {\n  width: inherit;\n  height: inherit;\n}\n\n</style>\n"]}]}